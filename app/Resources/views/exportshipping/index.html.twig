{% extends 'base.html.twig' %}

{% block body %}
    <h1 class="text-center">Export shippings list</h1>
 <div class="table-responsive">
    <table class="table table-bordered table-hover table-striped">
        <thead>
            <tr>
            
                <th>Createdat</th>
                <th>File</th>
                
            </tr>
        </thead>
        <tbody>
        {% for exportShipping in exportShippings %}
            <tr>
                
                
                <td>{% if exportShipping.createdAt %}{{ exportShipping.createdAt|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ exportShipping.file }}</td>
               
            </tr>
        {% endfor %}
        </tbody>
    </table>
 </div>
    
{% endblock %}
